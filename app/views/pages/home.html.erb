<div class="container">
  <h1>Home</h1>

  <div class="parks">
    <% @parks.each do |park| %>
      <div class="parks-park">
        <h2><%= park.name %></h2>
        <% park.dogs.each do |dog| %>
          <%= render 'components/card_dog', dog: dog %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
